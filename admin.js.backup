/**
 * Controller de Autentica√ß√£o
 */
const AuthController = {
  init() {
    if (AuthService.isLoggedIn()) {
      this.showAdminPanel();
    } else {
      this.showLoginScreen();
    }
  },

  showLoginScreen() {
    UIHelper.show("login-screen");
    UIHelper.hide("admin-panel");
  },

  showAdminPanel() {
    UIHelper.hide("login-screen");
    UIHelper.show("admin-panel");
    ProductController.loadAll();
    ProductController.updateStats();
  },

  handleLogin(e) {
    e.preventDefault();

    const username = UIHelper.getValue("username");
    const password = UIHelper.getValue("password");
    const errorEl = document.getElementById("login-error");

    if (AuthService.login(username, password)) {
      this.showAdminPanel();
      NotificationHelper.show("‚úÖ Login realizado com sucesso!", "success");
    } else {
      errorEl.style.display = "block";
      setTimeout(() => errorEl.style.display = "none", 3000);
    }
  },

  handleLogout() {
    AuthService.logout();
    this.showLoginScreen();
    NotificationHelper.show("üëã Voc√™ saiu do sistema", "success");
  }
};

/**
 * Controller de Produtos
