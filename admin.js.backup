/**
 * Servi√ßo de Storage (LocalStorage)
 */
const StorageService = {
  get(key) {
    const data = localStorage.getItem(key);
    return data ? JSON.parse(data) : [];
  },

  set(key, value) {
    localStorage.setItem(key, JSON.stringify(value));
  },

  clear(key) {
    localStorage.removeItem(key);
  }
};

/**
 * Servi√ßo de Autentica√ß√£o
 */
const AuthService = {
  isLoggedIn() {
    return sessionStorage.getItem(CONFIG.SESSION_KEY) === "true";
  },

  login(username, password) {
    if (username === CONFIG.ADMIN_USER && password === CONFIG.ADMIN_PASSWORD) {
      sessionStorage.setItem(CONFIG.SESSION_KEY, "true");
      return true;
    }
    return false;
  },

  logout() {
    sessionStorage.removeItem(CONFIG.SESSION_KEY);
  }
};

/**
 * Servi√ßo de Produtos (CRUD)
 */
const ProductService = {
  getAll() {
    return StorageService.get(CONFIG.STORAGE_KEY);
  },

  save(products) {
    StorageService.set(CONFIG.STORAGE_KEY, products);
  },

  getById(id) {
    const products = this.getAll();
    return products.find(p => p.id === id);
  },

  add(productData) {
    const products = this.getAll();
    const product = new Product(productData);
    products.push(product);
    this.save(products);
    return product;
  },

  update(id, productData) {
    let products = this.getAll();
    products = products.map(p => 
      p.id === id ? { ...p, ...productData } : p
    );
    this.save(products);
    return this.getById(id);
  },

  delete(id) {
    let products = this.getAll();
    products = products.filter(p => p.id !== id);
    this.save(products);
  },

  toggleStatus(id) {
    let products = this.getAll();
    products = products.map(p => {
      if (p.id === id) {
        return { ...p, soldOut: !p.soldOut, status: !p.soldOut ? "sold-out" : "available" };
      }
      return p;
    });
    this.save(products);
    return this.getById(id);
  },

  filterByCategory(category) {
    const products = this.getAll();
    return category === "all" 
      ? products 
      : products.filter(p => p.category === category);
  },

  getStats() {
    const products = this.getAll();
    return {
      total: products.length,
      available: products.filter(p => !p.soldOut).length,
      soldOut: products.filter(p => p.soldOut).length
    };
  },

  generateId() {
    return `prod_${Date.now()}_${Math.random().toString(36).substr(2, 9)}`;
  },

  initialize(initialProducts) {
    const existing = StorageService.get(CONFIG.STORAGE_KEY);
    if (!existing || existing.length === 0) {
      console.log("üì¶ Importando produtos iniciais...");
      this.save(initialProducts);
      console.log(`‚úÖ ${initialProducts.length} produtos importados!`);
  }
};

